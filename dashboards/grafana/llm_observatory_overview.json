{
  "title": "LLM Observatory Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "sum(rate(llm_requests_total[1m]))",
          "legendFormat": "req/s"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error Rate",
      "targets": [
        {
          "expr": "sum(rate(llm_requests_total{status=\"error\"}[5m])) / clamp_min(sum(rate(llm_requests_total[5m])), 0.0001)",
          "legendFormat": "error_ratio"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "P95 Latency (ms)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_request_latency_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "p95_ms"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Upstream Errors",
      "targets": [
        {
          "expr": "increase(llm_upstream_errors_total[5m])",
          "legendFormat": "errors_5m"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ]
}
